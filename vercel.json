{
    "version": 2,
    "builds": [
        {
            "src": "backend/main.py",
            "use": "@vercel/python",
            "config": {
                "maxDuration": 10
            }
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "backend/main.py"
        }
    ]
}